<ul>
	<% @posts.each do |post| %>
		<li>
			<%= post.title %>
			<%= post.content %>
	    <%= link_to "Show", post_path(post) %>
	    <%= link_to 'Edit', edit_post_path(post) %>
	    <%= button_to 'Delete', post_path(post), :method => :delete, :data => { :confirm => "Do you want delete this post?" } %>
		</li>
	<% end %>
</ul>
	<%= link_to 'New Post', new_post_path, class: :'btn btn-danger' %>